<a href="/" class="btn btn-info" style="margin-top:5px;">Logout</a>
<h4>My Result</h4>
<div class="stuTable">
  <table class="table table-striped">
      <thead>
        <tr>
          <th scope="col">Roll no.</th>
          <th scope="col">Name</th>
          <th scope="col">Date of birth</th>
          <th scope="col">Score</th>
        </tr>
      </thead>
      <tbody>        
        <tr>
          <td><%= result[0].roll %> </td>
          <td><%= result[0].sname %> </td>
          <td><%= result[0].dob %> </td>
          <td><%= result[0].score %> </td>
        </tr>
        
      </tbody>
    </table>    
</div>